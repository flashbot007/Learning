{
  "name": "veet",
  "description": "Veet - Beautiful, fast and modern Tailwind UI Library for Vue.js",
  "version": "1.0.5",
  "type": "module",
  "files": [
    "dist",
    "public"
  ],
  "main": "dist/veet.umd.js",
  "unpkg": "dist/veet.iife.js",
  "jsdelivr": "dist/veet.iife.js",
  "module": "./dist/veet.es.js",
  "exports": {
    ".": {
      "import": "./dist/veet.es.js",
      "require": "./dist/veet.umd.js"
    }
  },
  "types": "./dist/types/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/veetuse/veet.git"
  },
  "keywords": [
    "vue",
    "veet",
    "ui",
    "component"
  ],
  "author": "Veet",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/veetuse/veet/discussions"
  },
  "homepage": "https://github.com/veetuse/veet#readme",
  "scripts": {
    "dev": "vite",
    "build": "vite build && tsc --emitDeclarationOnly && mv dist/src dist/types && rm dist/tsconfig.tsbuildinfo",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "node scripts/release.cjs"
  },
  "dependencies": {
    "vue": "3.2.37"
  },
  "devDependencies": {
    "@types/node": "18.7.6",
    "@vitejs/plugin-vue": "3.0.3",
    "autoprefixer": "10.4.8",
    "chalk": "5.0.1",
    "conventional-changelog-cli": "2.2.2",
    "enquirer": "2.3.6",
    "execa": "6.1.0",
    "iconify-icon": "1.0.0-beta.3",
    "postcss": "8.4.16",
    "prettier": "^2.7.1",
    "sass": "1.54.5",
    "semver": "7.3.7",
    "tailwindcss": "3.1.8",
    "typescript": "4.7.4",
    "vite": "3.0.9",
    "vue-tsc": "0.40.1"
  }
}
